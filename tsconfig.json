{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "lib": ["ES2015", "DOM"],
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
